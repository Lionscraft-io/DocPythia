{
  "name": "docpythia",
  "version": "1.0.0",
  "description": "AI-powered documentation management system that monitors community discussions and automatically generates documentation updates",
  "type": "module",
  "license": "MIT",
  "author": "Lionscraft <info@lionscraft.io>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lionscraft-io/DocPythia.git"
  },
  "homepage": "https://github.com/Lionscraft-io/DocPythia#readme",
  "bugs": {
    "url": "https://github.com/Lionscraft-io/DocPythia/issues"
  },
  "keywords": [
    "documentation",
    "ai",
    "llm",
    "gemini",
    "rag",
    "docs-management",
    "automated-docs"
