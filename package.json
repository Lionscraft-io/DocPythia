  "scripts": {
    "dev": "NODE_ENV=development npx tsx server/index.ts",
    "build": "vite build --config vite.config.production.ts && node esbuild.config.js",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "dev:docker": "docker run -d --name docpythia-local -p 3000:8080 --env-file .env docpythia-local",
    "kill-port": "bash scripts/permanent/kill-port.sh",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
