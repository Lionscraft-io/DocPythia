{
  "project": {
    "name": "ConfluxDocs AI",
    "shortName": "conflux",
    "description": "AI-powered documentation assistant for Conflux Network",
    "domain": "confluxdocs.ai",
    "supportEmail": "support@conflux-chain.org"
  },
  "branding": {
    "logo": "https://confluxnetwork.org/images/logo.svg",
    "favicon": "https://confluxnetwork.org/favicon.ico",
    "primaryColor": "#00A3FF",
    "secondaryColor": "#0D0D0D",
    "accentColor": "#4ECDC4",
    "darkModePrimaryColor": "#00A3FF",
    "projectUrl": "https://confluxnetwork.org"
  },
  "documentation": {
    "gitUrl": "https://github.com/Conflux-Chain/conflux-documentation",
    "branch": "main",
    "docsPath": "docs"
  },
  "community": {
    "zulip": {
      "enabled": false
    },
    "telegram": {
      "enabled": true,
      "botToken": "your-telegram-bot-token",
      "channelId": "@conflux_english"
    },
    "discord": {
      "enabled": true,
      "botToken": "your-discord-bot-token",
      "guildId": "your-guild-id",
      "channelId": "your-channel-id"
    }
  },
  "widget": {
    "enabled": true,
    "title": "Conflux Assistant",
    "welcomeMessage": "Hello! I'm your Conflux Network documentation assistant. How can I help you today?",
    "suggestedQuestions": [
      "How do I deploy a smart contract on Conflux?",
      "What is the difference between Core Space and eSpace?",
      "How do I set up a Conflux node?",
      "What are the transaction fees on Conflux?"
    ],
    "position": "bottom-right",
    "theme": "auto",
    "primaryColor": "#00A3FF"
  },
  "features": {
    "ragEnabled": true,
    "schedulerEnabled": false,
    "chatEnabled": true,
    "analyticsEnabled": false,
    "versionHistoryEnabled": true
  },
  "admin": {
    "token": "change_me_in_production",
    "allowedOrigins": [
      "http://localhost:3762",
      "http://localhost:5173",
      "https://confluxdocs.ai"
    ]
  }
}
